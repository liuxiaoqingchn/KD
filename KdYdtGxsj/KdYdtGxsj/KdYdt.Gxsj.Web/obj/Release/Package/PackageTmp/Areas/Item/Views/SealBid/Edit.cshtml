@model   PrjSealBid
@using (Html.BbBeginForm())
{
    using (@Html.BeginPageTitle())
    {
        @Html.BbBtnSubmit()
        @Html.BbBtnLinkReturn()
    }

    @Html.NavBreadcrumb()
    @Html.AntiForgeryToken()
    @Html.HiddenFor(x => x.Id)
    @Html.ValidationSummary(true)

    <div class="panel panel-info">
        <div class="panel-heading" data-collapse="false">
            <span class="panel-title">封标信息</span>
        </div>
        <div class="panel-body" id="content"> 
            <div class="row">
                @Html.BbDlgPickerFor(x => x.PrjName, "SelectProject", x => x.ProjectId, 1, "#content", 10, "项目名称", new { controller = "SelectProject" }, null, null)
            </div>  
            <div class="row">
                @Html.BbUserPickerFor(x=>x.CheckPersonId,x=>x.CheckPersonName)
                @Html.BbTextBoxFor(x => x.CheckDate, 4)
            </div>
            <div class="row">
                @Html.BbTextBoxFor(x => x.Explain, 10)
            </div>
            @Html.RowModifier()  
            @Html.AnnexUnit()
            
        </div>
    </div>
}
@Html.AnnexUpload()