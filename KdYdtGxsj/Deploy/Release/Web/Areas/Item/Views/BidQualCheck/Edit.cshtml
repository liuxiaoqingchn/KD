@model  BidQualCheck
@using (Html.BbBeginForm())
{
    using (@Html.BeginPageTitle())
    {
        @Html.BbBtnSubmit()
        @Html.BbBtnLinkReturn()
    }

    @Html.NavBreadcrumb()
    @Html.AntiForgeryToken()
    @Html.HiddenFor(x => x.Id)
    @Html.ValidationSummary(true)

    <div class="panel panel-info">
        <div class="panel-heading" data-collapse="false">
            <span class="panel-title">资格审查结果</span>
        </div>
        <div class="panel-body" >
            <div class="row">
                @Html.SjPickPrjPlanFor(x => x.ProjectId, x => x.PrjName, 1)
                @*@Html.BbDlgPickerFor(x => x.ProjectId, "PrjSignPick", x => x.PrjName, 1, "#content", 4, "项目名称", new { controller = "PrjPicks" }, null, null)*@
                @Html.BbTextBoxFor(x => x.PrjCode)
                @Html.HiddenFor(x => x.PrjCode)
            </div>
            <div class="row">
                @Html.BbRadioListFor(x => x.QualCheckType)
                @Html.BbTextBoxFor(x => x.QualDate, 4)
            </div>
            <div class="row">
                @Html.BbUserPickFor(x => x.CheckPersonId, x => x.CheckPersonName, 1)
                @Html.BbTextBoxFor(x => x.CheckDate, 4)
            </div>
            <div class="row">
                @Html.BbTextBoxFor(x => x.Explain, 10)
            </div>
            @Html.RowModifier()
            @Html.AnnexUnit()

        </div>
    </div>
}
@Html.AnnexUpload()