
@model  List<PrjPraise>
<div class="panel panel-info">
    <div class="panel-heading" @Html.DataCollapse(true)>
        <span class="panel-title">获奖信息</span>
    </div>
    @using (Html.BeginDataTable())
    {
        int i = 1;
        @Html.BbTHeadRow("", "奖项名称", "获奖工程", "奖项等级", "获奖时间")
        if (!Model.IsNull())
        {
            using (Html.BeginDataTableBody())
            {
                foreach (PrjPraise item in Model)
                {
                    using (Html.BeginDataTableRow(item, false))
                    {
                        @Html.TdNumCenter(i)
                        @Html.TdNumSn(Html.BbLnkDlgAction("PraiseView", item.PraiseName, new { id = item.Id }, IconAwesome.NoneKD, "点击查看详情"))
                        @Html.TdNumSn(item.PraiseProject)
                        @Html.TdNumSn(item.PraiseType)
                        @Html.TdNumSn(item.PraiseDate)
                    }
                    i++;
                }
            }
        }
    }
</div>